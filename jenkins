pipeline {
    agent any 

    tools {
        maven 'MAVEN_PATH'
    }

    stages {
        stage ('Load Tools'){
            steps {
                sh "mvn -version"
            }
        }
    }
}

node {
        sh 'ls -l'
        dir ('foo'){
            writeFile file:'dummy', text:''
        }
        sh 'ls -l'
    }
